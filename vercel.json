{
  "framework": "angular",
  "functions": {
    "api/employee-management/[...segments].js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/employee-management/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}
